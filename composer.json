{
    "name": "sms/projetoModelo",
    "description": "Projeto modelo para as aplicações da SMS",
    "keywords": ["modelo", "projeto", "sms", "project template"],
    "homepage": "saudecaruaru.pe.gov.br",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors":{
        "name":"Júnior Pires",
        "email":"juniorpiresupe@gmail.com",
        "role":"Developer"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-grid": "@dev",
        "dmstr/yii2-adminlte-asset": "2.*",
        "dektrium/yii2-user": "0.9.*@dev",
        "johnitvn/yii2-rbac-plus": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
